<html>

<head>
    <title>Todo App</title>
    <link rel='stylesheet' href='styles.css'>
</head>

<body>
    <div class="box" id="heading">
        <h1>
            <%= keyOfTitle %>
        </h1>
    </div>
    <div class="box">
        <% for(i=0; i<keyOfList.length; i++){ %>
        <div class="item">
            <input type="checkbox">
            <p><%= keyOfList[i] %></p>
        </div>
        <% } %>
        <form class="item" action='/' method='post'>
            <input type='text' placeholder='New item' name='valueOfList' autofocus>
            <button type="submit">+</button>
        </form>
    </div>
</body>

</html>